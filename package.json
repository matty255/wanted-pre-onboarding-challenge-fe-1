{
  "name": "owl",
  "version": "1.0.0",
  "description": "원티드 프리온보딩 챌린지 프론트엔드",
  "main": "index.ts",
  "scripts": {
    "express": "cd server && nodemon --ignore db/ --watch '*.ts' --exec node --experimental-modules --es-module-specifier-resolution=node --loader ts-node/esm index.ts",
    "react": "cd client && npm start",
    "start": "concurrently \"npm run express\" \"npm run react\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matty255/wanted-pre-onboarding-challenge-fe-1.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/matty255/wanted-pre-onboarding-challenge-fe-1/issues"
  },
  "homepage": "https://github.com/matty255/wanted-pre-onboarding-challenge-fe-1#readme",
  "dependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.2",
    "concurrently": "^7.3.0",
    "express": "^4.18.1",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "nodemon": "^2.0.19"
  }
}
